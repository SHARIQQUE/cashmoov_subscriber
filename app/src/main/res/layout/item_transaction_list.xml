<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    app:cardElevation="0dp"
    app:cardCornerRadius="@dimen/_6sdp"
    android:layout_margin="@dimen/_5sdp"
    app:cardBackgroundColor="@color/white">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:padding="@dimen/_10sdp">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal"
            android:weightSum="1">
            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:text="@string/trans_id"
                android:textColor="@color/textcolorheading"
                android:textSize="@dimen/_11sdp"
                android:textStyle="bold"
                android:layout_weight="0.5"/>
            <View
                android:layout_width="1dp"
                android:layout_height="match_parent"
                android:background="@color/view_color"
                android:layout_marginStart="@dimen/_10sdp"
                android:layout_marginEnd="@dimen/_10sdp"/>
            <TextView
                android:id="@+id/transId"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:text="N/A"
                android:textColor="@color/textcolor"
                android:textSize="@dimen/_11sdp"
                android:layout_weight="0.5"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal"
            android:layout_marginTop="@dimen/_5sdp"
            android:weightSum="1">
            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:text="@string/source"
                android:textColor="@color/textcolorheading"
                android:textSize="@dimen/_11sdp"
                android:textStyle="bold"
                android:layout_weight="0.5"/>
            <View
                android:layout_width="1dp"
                android:layout_height="match_parent"
                android:background="@color/view_color"
                android:layout_marginStart="@dimen/_10sdp"
                android:layout_marginEnd="@dimen/_10sdp"/>
            <TextView
                android:id="@+id/source"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:text="N/A"
                android:textColor="@color/textcolor"
                android:textSize="@dimen/_11sdp"
                android:layout_weight="0.5"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal"
            android:layout_marginTop="@dimen/_5sdp"
            android:weightSum="1">

            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="0.5"
                android:text="@string/destination"
                android:textColor="@color/textcolorheading"
                android:textSize="@dimen/_11sdp"
                android:textStyle="bold" />
            <View
                android:layout_width="1dp"
                android:layout_height="match_parent"
                android:background="@color/view_color"
                android:layout_marginStart="@dimen/_10sdp"
                android:layout_marginEnd="@dimen/_10sdp"/>
            <TextView
                android:id="@+id/destination"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:text="N/A"
                android:textColor="@color/textcolor"
                android:textSize="@dimen/_11sdp"
                android:layout_weight="0.5"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal"
            android:layout_marginTop="@dimen/_5sdp"
            android:weightSum="1">
            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:text="@string/source_msisdn"
                android:textColor="@color/textcolorheading"
                android:textSize="@dimen/_11sdp"
                android:textStyle="bold"
                android:layout_weight="0.5"/>
            <View
                android:layout_width="1dp"
                android:layout_height="match_parent"
                android:background="@color/view_color"
                android:layout_marginStart="@dimen/_10sdp"
                android:layout_marginEnd="@dimen/_10sdp"/>
            <TextView
                android:id="@+id/sourceMSISDN"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:text="N/A"
                android:textColor="@color/textcolor"
                android:textSize="@dimen/_11sdp"
                android:layout_weight="0.5"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal"
            android:layout_marginTop="@dimen/_5sdp"
            android:weightSum="1">
            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:text="@string/dest_msisdn"
                android:textColor="@color/textcolorheading"
                android:textSize="@dimen/_11sdp"
                android:textStyle="bold"
                android:layout_weight="0.5"/>
            <View
                android:layout_width="1dp"
                android:layout_height="match_parent"
                android:background="@color/view_color"
                android:layout_marginStart="@dimen/_10sdp"
                android:layout_marginEnd="@dimen/_10sdp"/>
            <TextView
                android:id="@+id/destMSISDN"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:text="N/A"
                android:textColor="@color/textcolor"
                android:textSize="@dimen/_11sdp"
                android:layout_weight="0.5"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal"
            android:layout_marginTop="@dimen/_5sdp"
            android:weightSum="1">
            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:text="@string/recharge_no"
                android:textColor="@color/textcolorheading"
                android:textSize="@dimen/_11sdp"
                android:textStyle="bold"
                android:layout_weight="0.5"/>
            <View
                android:layout_width="1dp"
                android:layout_height="match_parent"
                android:background="@color/view_color"
                android:layout_marginStart="@dimen/_10sdp"
                android:layout_marginEnd="@dimen/_10sdp"/>
            <TextView
                android:id="@+id/rechargeNumber"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:text="N/A"
                android:textColor="@color/textcolor"
                android:textSize="@dimen/_11sdp"
                android:layout_weight="0.5"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal"
            android:layout_marginTop="@dimen/_5sdp"
            android:weightSum="1">
            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:text="@string/transaction_type"
                android:textColor="@color/textcolorheading"
                android:textSize="@dimen/_11sdp"
                android:textStyle="bold"
                android:layout_weight="0.5"/>
            <View
                android:layout_width="1dp"
                android:layout_height="match_parent"
                android:background="@color/view_color"
                android:layout_marginStart="@dimen/_10sdp"
                android:layout_marginEnd="@dimen/_10sdp"/>
            <TextView
                android:id="@+id/transType"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:text="N/A"
                android:textColor="@color/textcolor"
                android:textSize="@dimen/_11sdp"
                android:layout_weight="0.5"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal"
            android:layout_marginTop="@dimen/_5sdp"
            android:weightSum="1">
            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:text="@string/amounts"
                android:textColor="@color/textcolorheading"
                android:textSize="@dimen/_11sdp"
                android:textStyle="bold"
                android:layout_weight="0.5"/>
            <View
                android:layout_width="1dp"
                android:layout_height="match_parent"
                android:background="@color/view_color"
                android:layout_marginStart="@dimen/_10sdp"
                android:layout_marginEnd="@dimen/_10sdp"/>
            <TextView
                android:id="@+id/amount"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:text="N/A"
                android:textColor="@color/textcolor"
                android:textSize="@dimen/_11sdp"
                android:layout_weight="0.5"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal"
            android:layout_marginTop="@dimen/_5sdp"
            android:weightSum="1">
            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:text="@string/fee"
                android:textColor="@color/textcolorheading"
                android:textSize="@dimen/_11sdp"
                android:textStyle="bold"
                android:layout_weight="0.5"/>
            <View
                android:layout_width="1dp"
                android:layout_height="match_parent"
                android:background="@color/view_color"
                android:layout_marginStart="@dimen/_10sdp"
                android:layout_marginEnd="@dimen/_10sdp"/>
            <TextView
                android:id="@+id/fee"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:text="N/A"
                android:textColor="@color/textcolor"
                android:textSize="@dimen/_11sdp"
                android:layout_weight="0.5"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal"
            android:layout_marginTop="@dimen/_5sdp"
            android:weightSum="1">
            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:text="@string/tax_type"
                android:textColor="@color/textcolorheading"
                android:textSize="@dimen/_11sdp"
                android:textStyle="bold"
                android:layout_weight="0.5"/>
            <View
                android:layout_width="1dp"
                android:layout_height="match_parent"
                android:background="@color/view_color"
                android:layout_marginStart="@dimen/_10sdp"
                android:layout_marginEnd="@dimen/_10sdp"/>
            <TextView
                android:id="@+id/taxType"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:text="N/A"
                android:textColor="@color/textcolor"
                android:textSize="@dimen/_11sdp"
                android:layout_weight="0.5"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal"
            android:layout_marginTop="@dimen/_5sdp"
            android:weightSum="1">
            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:text="@string/tax"
                android:textColor="@color/textcolorheading"
                android:textSize="@dimen/_11sdp"
                android:textStyle="bold"
                android:layout_weight="0.5"/>
            <View
                android:layout_width="1dp"
                android:layout_height="match_parent"
                android:background="@color/view_color"
                android:layout_marginStart="@dimen/_10sdp"
                android:layout_marginEnd="@dimen/_10sdp"/>
            <TextView
                android:id="@+id/tax"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:text="N/A"
                android:textColor="@color/textcolor"
                android:textSize="@dimen/_11sdp"
                android:layout_weight="0.5"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal"
            android:layout_marginTop="@dimen/_5sdp"
            android:weightSum="1">
            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:text="@string/trans_reversed"
                android:textColor="@color/textcolorheading"
                android:textSize="@dimen/_11sdp"
                android:textStyle="bold"
                android:layout_weight="0.5"/>
            <View
                android:layout_width="1dp"
                android:layout_height="match_parent"
                android:background="@color/view_color"
                android:layout_marginStart="@dimen/_10sdp"
                android:layout_marginEnd="@dimen/_10sdp"/>
            <TextView
                android:id="@+id/transReversed"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:text="N/A"
                android:textColor="@color/textcolor"
                android:textSize="@dimen/_11sdp"
                android:layout_weight="0.5"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal"
            android:layout_marginTop="@dimen/_5sdp"
            android:weightSum="1">
            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:text="@string/trans_date"
                android:textColor="@color/textcolorheading"
                android:textSize="@dimen/_11sdp"
                android:textStyle="bold"
                android:layout_weight="0.5"/>
            <View
                android:layout_width="1dp"
                android:layout_height="match_parent"
                android:background="@color/view_color"
                android:layout_marginStart="@dimen/_10sdp"
                android:layout_marginEnd="@dimen/_10sdp"/>
            <TextView
                android:id="@+id/transDate"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:text="N/A"
                android:textColor="@color/textcolor"
                android:textSize="@dimen/_11sdp"
                android:layout_weight="0.5"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal"
            android:layout_marginTop="@dimen/_5sdp"
            android:weightSum="1">
            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:text="@string/status"
                android:textColor="@color/textcolorheading"
                android:textSize="@dimen/_11sdp"
                android:textStyle="bold"
                android:layout_weight="0.5"/>
            <View
                android:layout_width="1dp"
                android:layout_height="match_parent"
                android:background="@color/view_color"
                android:layout_marginStart="@dimen/_10sdp"
                android:layout_marginEnd="@dimen/_10sdp"/>
            <TextView
                android:id="@+id/status"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:text="N/A"
                android:textColor="@color/textcolor"
                android:textSize="@dimen/_11sdp"
                android:layout_weight="0.5"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal"
            android:gravity="end"
            android:layout_marginTop="@dimen/_5sdp">
            <com.google.android.material.floatingactionbutton.FloatingActionButton
                android:id="@+id/fab_view"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:scaleType="center"
                app:srcCompat="@drawable/ic_view"
                app:fabSize="mini"
                android:backgroundTint="@color/white"
                app:borderWidth="0dp"
                android:elevation="@dimen/_6sdp"
                android:layout_margin="@dimen/_6sdp"/>
        </LinearLayout>

    </LinearLayout>
</androidx.cardview.widget.CardView>