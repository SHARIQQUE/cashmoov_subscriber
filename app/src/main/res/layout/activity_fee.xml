<?xml version="1.0" encoding="utf-8"?>
<androidx.core.widget.NestedScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activity.fee.Fee">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <include layout="@layout/toolbar_active" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_5sdp"
            android:layout_marginStart="@dimen/_15sdp"
            android:layout_marginEnd="@dimen/_15sdp"
            android:layout_marginBottom="@dimen/_15sdp"
            android:orientation="vertical">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/aileron_bold"
                android:lineSpacingExtra="3sp"
                android:text="@string/service_fees"
                android:textColor="#29304d"
                android:textSize="@dimen/_16sdp" />
            

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_15sdp"
            android:orientation="horizontal">

            <androidx.cardview.widget.CardView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                app:cardCornerRadius="@dimen/_10sdp"
                app:cardBackgroundColor="@color/bleu_cashmoov">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:gravity="center"
                    android:orientation="horizontal"
                    android:padding="@dimen/_10sdp">

                    <FrameLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content">
                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textSize="15sp"
                            android:textColor="#ffffff"
                            android:fontFamily="@font/aileron_bold"
                            android:layout_gravity="center"
                            android:gravity="center"
                            android:lineSpacingExtra="10sp"
                            app:srcCompat="@drawable/logo200x70"

                            />
                    </FrameLayout>
                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="12dp"
                        android:orientation="vertical"/>
                </LinearLayout>
            </androidx.cardview.widget.CardView>

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_20sdp"
            android:background="#F9F9F9"
            android:orientation="vertical">

            <LinearLayout
                android:id="@+id/linMoneyTransfer"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="#ffffff"
                android:orientation="horizontal"
                android:padding="@dimen/_10sdp">

                <ImageView
                    android:layout_width="@dimen/_30sdp"
                    android:layout_height="@dimen/_30sdp"
                    android:layout_gravity="center_vertical"
                    app:srcCompat="@drawable/ic_moneytransfert" />

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/_10sdp"
                    android:orientation="vertical"
                    android:layout_gravity="center_vertical">

                    <TextView
                        android:id="@+id/tvMoneyTransfer"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/aileron_bold"
                        android:layout_gravity="center_vertical"
                        android:lineSpacingExtra="10sp"
                        android:text="@string/money_transfer"
                        android:textColor="#3a3a3a"
                        android:textSize="@dimen/_13sdp" />

                        <TextView
                            android:id="@+id/tvFeeMoneyTransfer"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:fontFamily="@font/aileron_bold"
                            android:gravity="right"
                            android:lineSpacingExtra="10sp"
                            android:layout_marginTop="@dimen/_2sdp"
                            android:text="@string/free_service"
                            android:textColor="@color/bleu_cashmoov"
                            android:textSize="@dimen/_11sdp"
                            android:layout_gravity="center_vertical"/>

                </LinearLayout>
            </LinearLayout>


            <LinearLayout
                android:id="@+id/linAirtimePurhase"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:padding="@dimen/_10sdp"
                android:background="#ffffff"
                android:orientation="horizontal">

                <ImageView
                    android:layout_width="@dimen/_30sdp"
                    android:layout_height="@dimen/_30sdp"
                    app:srcCompat="@drawable/ic_rechargement"
                    android:layout_gravity="center_vertical" />


                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/_10sdp"
                    android:orientation="vertical"
                    android:layout_gravity="center_vertical">

                    <TextView
                        android:id="@+id/tvAirtimePurchase"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/aileron_bold"
                        android:layout_gravity="center_vertical"
                        android:lineSpacingExtra="10sp"
                        android:text="@string/airtime_purchase"
                        android:textColor="#3a3a3a"
                        android:textSize="@dimen/_13sdp" />

                        <TextView
                            android:id="@+id/tvFeeAirtimePurchase"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:fontFamily="@font/aileron_bold"
                            android:gravity="right"
                            android:lineSpacingExtra="10sp"
                            android:layout_marginTop="@dimen/_2sdp"
                            android:text="@string/free_service"
                            android:textColor="@color/bleu_cashmoov"
                            android:textSize="@dimen/_11sdp"
                            android:layout_gravity="center_vertical"/>

                    </LinearLayout>
            </LinearLayout>


            <LinearLayout
                android:id="@+id/linBillPay"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:padding="@dimen/_10sdp"
                android:background="#ffffff"
                android:orientation="horizontal">

                <ImageView
                    android:layout_width="@dimen/_30sdp"
                    android:layout_height="@dimen/_30sdp"
                    app:srcCompat="@drawable/ic_baseline_payment_24"
                    android:layout_gravity="center_vertical"/>


                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/_10sdp"
                    android:orientation="vertical"
                    android:layout_gravity="center_vertical">

                    <TextView
                        android:id="@+id/tvBillPayment"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/aileron_bold"
                         android:layout_gravity="center_vertical"
                        android:lineSpacingExtra="10sp"
                        android:text="@string/bill_payment"
                        android:textColor="#3a3a3a"
                        android:textSize="@dimen/_13sdp" />

                        <TextView
                            android:id="@+id/tvFeeBillPayment"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:fontFamily="@font/aileron_bold"
                            android:gravity="right"
                            android:lineSpacingExtra="10sp"
                            android:layout_marginTop="@dimen/_2sdp"
                            android:text="@string/free_service"
                            android:textColor="@color/bleu_cashmoov"
                            android:textSize="@dimen/_11sdp"
                            android:layout_gravity="center_vertical"/>

                    </LinearLayout>
            </LinearLayout>


            <LinearLayout
                android:id="@+id/linPay"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:padding="@dimen/_10sdp"
                android:background="#ffffff"
                android:orientation="horizontal">

                <ImageView
                    android:layout_width="@dimen/_30sdp"
                    android:layout_height="@dimen/_30sdp"
                    app:srcCompat="@drawable/ic_paimentfacture"
                    android:layout_gravity="center_vertical"/>
                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/_10sdp"
                    android:orientation="vertical"
                    android:layout_gravity="center_vertical">

                    <TextView
                        android:id="@+id/tvPay"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/aileron_bold"
                        android:layout_gravity="center_vertical"
                        android:lineSpacingExtra="10sp"
                        android:text="@string/pay"
                        android:textColor="#3a3a3a"
                        android:textSize="@dimen/_13sdp" />

                        <TextView
                            android:id="@+id/tvFeePay"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:fontFamily="@font/aileron_bold"
                            android:gravity="right"
                            android:lineSpacingExtra="10sp"
                            android:layout_marginTop="@dimen/_2sdp"
                            android:text="@string/free_service"
                            android:textColor="@color/bleu_cashmoov"
                            android:textSize="@dimen/_11sdp"
                            android:layout_gravity="center_vertical"/>

                </LinearLayout>
            </LinearLayout>


            <LinearLayout
                android:id="@+id/linCashWithdrawal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:padding="@dimen/_10sdp"
                android:background="#ffffff"
                android:orientation="horizontal">

                <ImageView
                    android:layout_width="@dimen/_30sdp"
                    android:layout_height="@dimen/_30sdp"
                    app:srcCompat="@drawable/ic_paymane24"
                    android:layout_gravity="center_vertical"/>

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/_10sdp"
                    android:orientation="vertical"
                    android:layout_gravity="center_vertical">

                    <TextView
                        android:id="@+id/tvCashWithdrawal"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/aileron_bold"
                         android:layout_gravity="center_vertical"
                        android:lineSpacingExtra="10sp"
                        android:text="@string/cash_withdrawal"
                        android:textColor="#3a3a3a"
                        android:textSize="@dimen/_13sdp" />

                        <TextView
                            android:id="@+id/tvFeeCashWithdrawal"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:fontFamily="@font/aileron_bold"
                            android:gravity="right"
                            android:lineSpacingExtra="10sp"
                            android:layout_marginTop="@dimen/_2sdp"
                            android:text="@string/free_service"
                            android:textColor="@color/bleu_cashmoov"
                            android:textSize="@dimen/_11sdp"
                            android:layout_gravity="center_vertical"/>

                    </LinearLayout>
            </LinearLayout>


            <LinearLayout
                android:id="@+id/linRecRemittance"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:padding="@dimen/_10sdp"
                android:background="#ffffff"
                android:orientation="horizontal">

                <ImageView
                    android:layout_width="@dimen/_30sdp"
                    android:layout_height="@dimen/_30sdp"
                    app:srcCompat="@drawable/ic_cashpickup24"
                    android:layout_gravity="center_vertical"/>


                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/_10sdp"
                    android:orientation="vertical"
                    android:layout_gravity="center_vertical">

                    <TextView
                        android:id="@+id/tvReceiveRemit"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/aileron_bold"
                         android:layout_gravity="center_vertical"
                        android:lineSpacingExtra="10sp"
                        android:text="@string/receive_remittance"
                        android:textColor="#3a3a3a"
                        android:textSize="@dimen/_13sdp" />

                        <TextView
                            android:id="@+id/tvFeeReceiveRemit"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:fontFamily="@font/aileron_bold"
                            android:gravity="right"
                            android:lineSpacingExtra="10sp"
                            android:layout_marginTop="@dimen/_2sdp"
                            android:text="@string/free_service"
                            android:textColor="@color/bleu_cashmoov"
                            android:textSize="@dimen/_11sdp"
                            android:layout_gravity="center_vertical"/>

                    </LinearLayout>
                </LinearLayout>


        </LinearLayout>
    </LinearLayout>
    </LinearLayout>

</androidx.core.widget.NestedScrollView>