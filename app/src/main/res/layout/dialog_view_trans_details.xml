              android:paddingTop="@dimen/_5sdp"
            android:paddingBottom="@dimen/_5sdp"
            android:textColor="@color/textcolor"
            android:textSize="@dimen/_11sdp"
            android:background="@drawable/btn_edittextname"
            android:backgroundTint="@color/bgColorHint"
             android:paddingStart="@dimen/_10sdp"
            android:paddingEnd="@dimen/_10sdp"
            android:gravity="center_vertical"
            android:layout_marginTop="@dimen/_5sdp"
            android:inputType="text"
            android:enabled="false"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/tax_type"
                        android:textColor="@color/textcolorheading"
            android:layout_marginStart="@dimen/_4sdp"

            android:textSize="@dimen/_11sdp"
            android:layout_marginTop="@dimen/_10sdp"/>
        <TextView
            android:id="@+id/etTaxType"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
              android:paddingTop="@dimen/_5sdp"
            android:paddingBottom="@dimen/_5sdp"
            android:textColor="@color/textcolor"
            android:textSize="@dimen/_11sdp"
            android:background="@drawable/btn_edittextname"
            android:backgroundTint="@color/bgColorHint"
             android:paddingStart="@dimen/_10sdp"
            android:paddingEnd="@dimen/_10sdp"
            android:gravity="center_vertical"
            android:layout_marginTop="@dimen/_5sdp"
            android:inputType="text"
            android:enabled="false"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/tax"
                        android:textColor="@color/textcolorheading"
            android:layout_marginStart="@dimen/_4sdp"

            android:textSize="@dimen/_11sdp"
            android:layout_marginTop="@dimen/_10sdp"/>
        <TextView
            android:id="@+id/etTax"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
              android:paddingTop="@dimen/_5sdp"
            android:paddingBottom="@dimen/_5sdp"
            android:textColor="@color/textcolor"
            android:textSize="@dimen/_11sdp"
            android:background="@drawable/btn_edittextname"
            android:backgroundTint="@color/bgColorHint"
             android:paddingStart="@dimen/_10sdp"
            android:paddingEnd="@dimen/_10sdp"
            android:gravity="center_vertical"
            android:layout_marginTop="@dimen/_5sdp"
            android:inputType="text"
            android:enabled="false"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/post_balance"
                        android:textColor="@color/textcolorheading"
            android:layout_marginStart="@dimen/_4sdp"

            android:textSize="@dimen/_11sdp"
            android:layout_marginTop="@dimen/_10sdp"/>
        <TextView
            android:id="@+id/etPostBalance"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
              android:paddingTop="@dimen/_5sdp"
            android:paddingBottom="@dimen/_5sdp"
            android:textColor="@color/textcolor"
            android:textSize="@dimen/_11sdp"
            android:background="@drawable/btn_edittextname"
            android:backgroundTint="@color/bgColorHint"
             android:paddingStart="@dimen/_10sdp"
            android:paddingEnd="@dimen/_10sdp"
            android:gravity="center_vertical"
