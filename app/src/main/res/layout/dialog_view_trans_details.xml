<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/colorPrimary"
        android:padding="@dimen/_10sdp">

        <TextView
            android:id="@+id/txt_header"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_centerVertical="true"
            android:layout_toLeftOf="@+id/img_close"
            android:text="@string/view_deatils"
            android:textColor="@color/white"
            android:textSize="@dimen/_13sdp"
            android:textStyle="bold" />

        <ImageView
            android:id="@+id/img_close"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_centerVertical="true"
            app:srcCompat="@drawable/ic_close" />
    </RelativeLayout>
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:fillViewport="true"
        android:scrollbars="none">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:padding="@dimen/_15sdp"
        android:background="@color/white">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/trans_id"
            android:textColor="@color/textcolorheading"
            android:layout_marginStart="@dimen/_4sdp"
            android:textSize="@dimen/_11sdp"  />
        <TextView
            android:id="@+id/etTransId"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textColor="@color/textcolor"
            android:textSize="@dimen/_11sdp"
            android:background="@drawable/btn_edittextname"
            android:backgroundTint="@color/bgColorHint"
            android:paddingStart="@dimen/_10sdp"
            android:paddingEnd="@dimen/_10sdp"
            android:gravity="center_vertical"
            android:layout_marginTop="@dimen/_5sdp"
            android:inputType="number"
            android:enabled="false"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/transaction_type"
                        android:textColor="@color/textcolorheading"
            android:layout_marginStart="@dimen/_4sdp"

            android:textSize="@dimen/_11sdp"
            android:layout_marginTop="@dimen/_10sdp"/>
        <TextView
            android:id="@+id/etTransType"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingTop="@dimen/_5sdp"
            android:paddingBottom="@dimen/_5sdp"
            android:textColor="@color/textcolor"
            android:textSize="@dimen/_11sdp"
            android:background="@drawable/btn_edittextname"
            android:backgroundTint="@color/bgColorHint"
            android:paddingStart="@dimen/_10sdp"
            android:paddingEnd="@dimen/_10sdp"
            android:gravity="center_vertical"
            android:layout_marginTop="@dimen/_5sdp"
            android:inputType="text"
            android:enabled="false"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/trans_date"
                        android:textColor="@color/textcolorheading"
            android:layout_marginStart="@dimen/_4sdp"

            android:textSize="@dimen/_11sdp"
            android:layout_marginTop="@dimen/_10sdp"/>
        <TextView
            android:id="@+id/etTransDate"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
              android:paddingTop="@dimen/_5sdp"
            android:paddingBottom="@dimen/_5sdp"
            android:textColor="@color/textcolor"
            android:textSize="@dimen/_11sdp"
            android:background="@drawable/btn_edittextname"
            android:backgroundTint="@color/bgColorHint"
             android:paddingStart="@dimen/_10sdp"
            android:paddingEnd="@dimen/_10sdp"
            android:gravity="center_vertical"
            android:layout_marginTop="@dimen/_5sdp"
            android:inputType="text"
            android:enabled="false"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/source"
                        android:textColor="@color/textcolorheading"
            android:layout_marginStart="@dimen/_4sdp"

            android:textSize="@dimen/_11sdp"
            android:layout_marginTop="@dimen/_10sdp"/>
        <TextView
            android:id="@+id/etSource"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
              android:paddingTop="@dimen/_5sdp"
            android:paddingBottom="@dimen/_5sdp"
            android:textColor="@color/textcolor"
            android:textSize="@dimen/_11sdp"
            android:background="@drawable/btn_edittextname"
            android:backgroundTint="@color/bgColorHint"
             android:paddingStart="@dimen/_10sdp"
            android:paddingEnd="@dimen/_10sdp"
            android:gravity="center_vertical"
            android:layout_marginTop="@dimen/_5sdp"
            android:inputType="text"
            android:enabled="false"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/destination"
                        android:textColor="@color/textcolorheading"
            android:layout_marginStart="@dimen/_4sdp"

            android:textSize="@dimen/_11sdp"
            android:layout_marginTop="@dimen/_10sdp"/>
        <TextView
            android:id="@+id/etDestination"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
              android:paddingTop="@dimen/_5sdp"
            android:paddingBottom="@dimen/_5sdp"
            android:textColor="@color/textcolor"
            android:textSize="@dimen/_11sdp"
            android:background="@drawable/btn_edittextname"
            android:backgroundTint="@color/bgColorHint"
             android:paddingStart="@dimen/_10sdp"
            android:paddingEnd="@dimen/_10sdp"
            android:gravity="center_vertical"
            android:layout_marginTop="@dimen/_5sdp"
            android:inputType="text"
            android:enabled="false"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/source_msisdn"
                        android:textColor="@color/textcolorheading"
            android:layout_marginStart="@dimen/_4sdp"

            android:textSize="@dimen/_11sdp"
            android:layout_marginTop="@dimen/_10sdp"/>
        <TextView
            android:id="@+id/etSourcMSISDN"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
              android:paddingTop="@dimen/_5sdp"
            android:paddingBottom="@dimen/_5sdp"
            android:textColor="@color/textcolor"
            android:textSize="@dimen/_11sdp"
            android:background="@drawable/btn_edittextname"
            android:backgroundTint="@color/bgColorHint"
             android:paddingStart="@dimen/_10sdp"
            android:paddingEnd="@dimen/_10sdp"
            android:gravity="center_vertical"
            android:layout_marginTop="@dimen/_5sdp"
            android:inputType="numberDecimal"
            android:enabled="false"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/dest_msisdn"
                        android:textColor="@color/textcolorheading"
            android:layout_marginStart="@dimen/_4sdp"

            android:textSize="@dimen/_11sdp"
            android:layout_marginTop="@dimen/_10sdp"/>
        <TextView
            android:id="@+id/etDestMSISDN"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
              android:paddingTop="@dimen/_5sdp"
            android:paddingBottom="@dimen/_5sdp"
            android:textColor="@color/textcolor"
            android:textSize="@dimen/_11sdp"
            android:background="@drawable/btn_edittextname"
            android:backgroundTint="@color/bgColorHint"
             android:paddingStart="@dimen/_10sdp"
            android:paddingEnd="@dimen/_10sdp"
            android:gravity="center_vertical"
            android:layout_marginTop="@dimen/_5sdp"
